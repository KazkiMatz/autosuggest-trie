{
  "name": "autosuggest-trie",
  "version": "1.3.0",
  "description": "Trie implementation for autosuggest components",
  "main": "dist/autosuggest-trie/autosuggest-trie.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/moroshko/autosuggest-trie.git"
  },
  "author": "Misha Moroshko <michael.moroshko@gmail.com>",
  "bugs": {
    "url": "https://github.com/moroshko/autosuggest-trie/issues"
  },
  "homepage": "https://github.com/moroshko/autosuggest-trie",
  "scripts": {
    "lint": "jscs src --verbose --max-errors=3 --reporter=inline --esnext",
    "test": "mocha src --recursive --compilers js:babel/register",
    "dist": "babel src/autosuggest-trie/autosuggest-trie.js -o dist/autosuggest-trie/autosuggest-trie.js && babel src/intersection-with-limit/intersection-with-limit.js -o dist/intersection-with-limit/intersection-with-limit.js",
    "build": "npm run lint && npm test && npm run dist"
  },
  "keywords": [
    "autosuggest",
    "autocomplete",
    "auto suggest",
    "auto complete",
    "trie autosuggest",
    "trie autocomplete",
    "trie auto suggest",
    "trie auto complete",
    "autosuggest trie",
    "autocomplete trie",
    "auto suggest trie",
    "auto complete trie"
  ],
  "devDependencies": {
    "babel": "^5.4.7",
    "chai": "^3.0.0",
    "jscs": "^1.13.1",
    "mocha": "^2.2.5"
  },
  "license": "MIT"
}
